# SoilWise Web Dashboard Environment Variables
# Copy this file to .env.local and update the values

# API Configuration
VITE_API_URL=http://localhost:8080/api
VITE_API_VERSION=v1

# MQTT Configuration  
VITE_MQTT_BROKER_URL=ws://localhost:8083/mqtt
VITE_MQTT_USERNAME=soilwise
VITE_MQTT_PASSWORD=soilwise

# Feature Flags
VITE_OFFLINE_ENABLED=false
VITE_SYNC_ENABLED=false
VITE_EDGE_MODE=false
VITE_VOICE_ENABLED=true
VITE_AI_ENABLED=true

# Map Configuration
VITE_MAPS_API_KEY=your_maps_api_key_here

# Development
VITE_DEBUG=true
VITE_LOG_LEVEL=info
